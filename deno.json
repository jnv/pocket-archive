{
	"tasks": {
		"enqueue": "deno --env-file --allow-env --allow-net --allow-read=./_data/ --allow-write=./_data/ --unstable-kv main.ts --enqueue",
    "process": "deno --env-file --allow-env --allow-net --allow-read=./_data/ --allow-write=./_data/ --unstable-kv main.ts --process --no-enqueue",
    "clean": "rm -rf ./_data && mkdir -p ./_data && touch ./_data/.gitkeep"
	},
	"imports": {
		"@graphql-typed-document-node/core": "npm:@graphql-typed-document-node/core@^3.2.0",
		"@std/assert": "jsr:@std/assert@1",
		"@std/async": "jsr:@std/async@^1.0.13",
		"@std/cli": "jsr:@std/cli@^1.0.19",
		"@std/fs": "jsr:@std/fs@^1.0.18",
		"@std/path": "jsr:@std/path@^1.1.0",
		"graphql": "npm:graphql@^16.11.0",
		"graphql-request": "npm:graphql-request@^7.2.0",
		"p-queue": "npm:p-queue@^8.1.0"
	},
  "fmt": {
    "singleQuote": true
  }
}
